#include <iostream>
using namespace std;

int main ()
{
int arr[5] = {2, 4, 6, 8, 10};

cout << "Array elements:" << endl;
for (int i = 0; i < 5; i++){
   cout << arr[i] << "";
 }
cout << endl;

int sum = 0;
for (int i = 0; i < 5; i++){
 sum += arr[i];
 }
cout << "Sum of elements: " << sum << endl;
return 0;
}
